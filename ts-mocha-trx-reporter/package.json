{
  "name": "@microsoft/ts-mocha-trx-reporter",
  "repository":"https://github.com/microsoft/dev-tunnels.git",
  "version": "1.0.0",
  "main": "dist/src/index.js",
  "types": "dist/src/index.d.ts",
  "files": [
    "/dist/**/*.js",
    "/dist/**/*.d.ts"
  ],
  "dependencies": {
    "mocha": "^6.1.4",
    "uuid": "^3.4.0",
    "xmlbuilder": "^13.0.2"
  },
  "devDependencies": {
    "@types/node": "^12.0.5",
    "@types/uuid": "^3.4.5",
    "prettier": "^1.19.1",
    "typescript": "^4.7.4"
  },
  "scripts": {
    "prepack": "npm run build",
    "build": "tsc",
    "watch": "tsc -w",
    "format": "prettier --write \"src/**/*.{ts,tsx}\""
  }
}
