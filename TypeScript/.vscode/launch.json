    "version": "0.2.0",
    "configurations": [
        {
            "type": "pwa-node",
            "request": "launch",
            "name": "Launch Client Connection",
            "skipFiles": [
                "<node_internals>/**"
            ],
            "program": "${workspaceFolder}/out/lib/tunnels-test/connection.js",
            "outFiles": [
                "${workspaceFolder}/**/*.js"
            ]
        },
        {
            "type": "pwa-node",
            "request": "launch",
            "name": "Launch Host",
            "skipFiles": [
                "<node_internals>/**"
            ],
            "program": "${workspaceFolder}/out/lib/tunnels-test/host.js",
            "outFiles": [
                "${workspaceFolder}/**/*.js"
            ]
        }
    ]
}